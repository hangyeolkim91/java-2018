package com.day6;

import java.util.Arrays;

public class MyBubbleSort {

	private int[] arr;
	
	public MyBubbleSort(int[] ar){
		
		arr = Arrays.copyOf(ar, ar.length);
		
	}
	
	 private void sort(){
		 
		 for(int i=1; i<arr.length; i++){
			 for(int j=0; j<arr.length-i; j++){
				 if(arr[j] > arr[j+1]){
					 
				 }
			 }
		 }
	 }
}
