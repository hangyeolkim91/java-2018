package com.class3;

class SuperClass{
	private String title;
	protected double area;
	
	public SuperClass(){
		
	}
	
	public SuperClass(String title){
		this.title = title;
	}
	
	public void write(){
		System.out.println(title + ": "+ area);
	}
}

class Circle extends SuperClass{
	
	private int r;
	protected static final double PI=3.14;
	
}
class Rectab extends SuperClass{
	
}



