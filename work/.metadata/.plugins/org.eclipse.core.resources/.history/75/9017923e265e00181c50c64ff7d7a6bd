package com.day6;

public class Test1 {

	public static void main(String[] args) {
		
		// bubble sort
		
		int[] num = {36, 30 , 26 , 10 , 4};
		int temp;
		
		System.out.println("Source Data: ");
		for(int i=0; i<num.length; i++){
			System.out.printf("%4d", num[i]);
			
		}
		
		System.out.println();
	}

}
