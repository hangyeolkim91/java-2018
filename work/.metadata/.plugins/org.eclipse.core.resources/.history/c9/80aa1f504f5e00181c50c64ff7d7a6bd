package com.day6;

import java.util.Scanner;

public class TestCom {

	int w,h; //전역변수 ,  instance변수
	
	public void input(){
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("가로?");
		w = sc.nextInt();
		
		System.out.print("세로?");
		h = sc.nextInt();
		
		//return;
	}
	
	public int area(){
		
		return w*h;
	}
	
}
