package com.day4;

import java.util.Scanner;

public class Hw1 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		System.out.print("Á¤¼ö?");
		
		int n = sc.nextInt();
		int sum=0;
		
		for(int i=1; i<=n; i++){
			for(int j=1; j<=i; j++){
				sum +=j;
			}
		}

	}

}
