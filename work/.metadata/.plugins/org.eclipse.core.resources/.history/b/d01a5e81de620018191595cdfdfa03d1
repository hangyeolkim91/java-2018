package com.class4;

class A{
	public int x = 10;
	
	public A(){
		
	}
	
	public void print(){
		System.out.println("x : "+ x);
	}
}

class B extends A {
	
	public int y = 20;
	
	public B(){
		super();
	}
	
	public void write(){
		System.out.println("y : "+ y);
	}
}

public class Test1 {

	public static void main(String[] args) {

		B ob = new B();
		
		ob.write();
		ob.print();
	}

}
