package com.class3;

public class Test3 {
	
	int a =5;
	
	//초기화 블럭
	{
		System.out.println("초기화 블럭 : a " + a);
		a=10;
		System.out.println("초기화 블럭 : a " + a);
	}
	
	static int b;
	
	// static 초기화 블럭
	static{
		b = 10;
		System.out.println("static 초기화 블럭 b : " +b);
	}

	public static void main(String[] args) {

		Test3 ob1 =new Test3();
	}

}
