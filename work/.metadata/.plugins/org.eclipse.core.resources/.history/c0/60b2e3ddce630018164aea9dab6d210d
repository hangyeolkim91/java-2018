package com.class5;

/*
 *  추상 클래스
 *  메모리의 낭비 없이 클래스를 미리 설계
 *  메소드를 재정의 해서 사용
 */

abstract class ShapeClass{
	
	abstract void draw();
	
}

class Circle extends ShapeClass{
	
	@Override
	public void draw() {
		
	}
}

class Rectangle extends ShapeClass{
	
	@Override
	void draw() {
	}
}

class Triangle extends ShapeClass{
	
	@Override
	void draw() {
	}
}


public class Test7 {

	public static void main(String[] args) {

	}

}
