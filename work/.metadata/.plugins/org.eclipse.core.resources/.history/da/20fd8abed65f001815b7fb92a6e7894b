package com.class2;

class Rect{
	
	private int w,h;
	
	public void set(int w, int h){
		
		this.w = w;
		this.h = h;
		
	}
	
	public int area(){
		return w*h;
	}
	public int length(){
		return (w+h)*2;
	}
	
	public void print(int a, int l){
		System.out.println("가로 : "+w);
		System.out.println("가로 : "+h);
		System.out.println("가로 : "+a);
		System.out.println("가로 : "+l);
	}
}

public class Test4 {

	public static void main(String[] args) {

		
	}

}
