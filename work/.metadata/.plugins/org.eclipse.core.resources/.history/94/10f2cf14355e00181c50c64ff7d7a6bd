package com.day6;

import java.util.Arrays;

public class Test3 {

	public static void main(String[] args) {

		//배열의 배열
		
		int[][] arr = new int[6][6];
		int n=0;
		
		for(int i=0; i<arr.length-1; i++){
			
			for(int j=0; j<arr.length-1; j++){
				
				//System.out.println(i+" : "+ j);
				n++;
				arr[i][j] = n;
			}
		}
		
		
		
	}

}
