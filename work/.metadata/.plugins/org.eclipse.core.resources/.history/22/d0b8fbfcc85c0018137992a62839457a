package com.day5;

import java.util.Scanner;

public class Hw1 {

	public static void main(String[] args) {
		
		/* 1. 5개의 정수를 입력받아 입력받은 수를 출력하고 입력받은 수중
		 * 가장 큰수와 적은수를 출력
		 */
		
		Scanner sc = new Scanner(System.in);
		
		int[] n = new int[5];
		
		System.out.print("다섯 개의 정수 입력?");
		
		for(int i=0; i<5; i++){
			n[i] = sc.nextInt();
		}
		
		
	}

}
